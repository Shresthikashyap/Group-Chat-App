{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install mysql2"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
